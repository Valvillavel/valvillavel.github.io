{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "name": "customer"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "company": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "address": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "phone": {
      "type": "biginteger"
    },
    "mobile": {
      "type": "biginteger"
    },
    "workOrders": {
      "via": "customer",
      "collection": "work-order"
    }
  }
}
